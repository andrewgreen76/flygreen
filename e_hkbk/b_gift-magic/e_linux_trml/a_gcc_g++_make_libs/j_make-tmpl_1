
CC = gcc
CFLAGS = -std=c11 -Wall -fmax-errors=10 -Wextra
LFLAGS =
LIBS =
OBJFILES = main.o aux.o
MAIN = prog



# Targets : 

all: $(MAIN)

$(MAIN): $(OBJFILES)
	 $(CC) $(CFLAGS) -o $(MAIN) $(OBJFILES)

%.o: %.c
     $(CC) $(CFLAGS) -c -o $@ $<

clean: 
	rm -f $(OBJFILES) $(MAIN)

launch: prog
	./prog 

#